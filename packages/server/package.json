{
  "name": "server",
  "version": "0.1.0",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --forceExit",
    "build": "tsc --project .",
    "dev": "cross-env NODE_ENV=development nodemon src/app.ts",
    "start": "node dist/app.js",
    "validate": "prettier -w .",
    "mikro-orm": "cross-env NODE_ENV=development mikro-orm",
    "create:migration": "cross-env NODE_ENV=development mikro-orm migration:create",
    "create:migration:test": "cross-env NODE_ENV=test mikro-orm migration:up",
    "update:schema:test": "cross-env NODE_ENV=test mikro-orm schema:update --run",
    "up:migration": "mikro-orm migration:up",
    "update:schema": "mikro-orm schema:update --run"
  },
  "dependencies": {
    "@mikro-orm/cli": "^4.4.4",
    "@mikro-orm/core": "^4.4.4",
    "@mikro-orm/migrations": "^4.4.4",
    "@mikro-orm/postgresql": "^4.4.4",
    "@types/morgan": "^1.9.2",
    "argon2": "^0.27.1",
    "connect-redis": "^5.1.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@types/connect-redis": "0.0.14",
    "@types/express": "4.17.11",
    "@types/jest": "^26.0.21",
    "@types/node": "14.14.31",
    "@types/redis": "^2.8.28",
    "@types/supertest": "^2.0.10",
    "cross-env": "^7.0.3",
    "dotenv": "8.2.0",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "nodemon": "2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-node": "9.1.1",
    "typescript": "4.1.5"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
